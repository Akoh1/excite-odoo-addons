
<odoo>
    <record model="ir.ui.view" id="product_estate_tree">
      <field name="name">product.estate.tree</field>
      <field name="model">product.estate</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="address"/>
        </tree>
      </field>
    </record>

    <record id="product_estate_form" model="ir.ui.view">
        <field name="name">product.estate.form</field>
        <field name="model">product.estate</field>
        <field name="priority">2</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="address"/>
                        </group>
                    </group>

                </sheet>

            </form>
        </field>
    </record>

    <record model="ir.actions.act_window" id="product_estate_window">
      <field name="name">Estate</field>
      <field name="res_model">product.estate</field>
      <field name="view_mode">tree,form</field>
    </record>



    <menuitem name="Estate" id="product_estate_menu"
              parent="stock.menu_stock_config_settings"
              action="fhfl_sales_custom.product_estate_window"
              groups="stock.group_stock_user,stock.group_stock_manager"
              sequence="1"/>

<record model="ir.ui.view" id="property_type_tree">
      <field name="name">property.type.tree</field>
      <field name="model">property.type</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
         <!--  <field name="address"/> -->
        </tree>
      </field>
    </record>

    <record id="property_type_form" model="ir.ui.view">
        <field name="name">property.type.form</field>
        <field name="model">property.type</field>
        <field name="priority">2</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                           <!--  <field name="address"/> -->
                        </group>
                    </group>

                </sheet>

            </form>
        </field>
    </record>

    <record model="ir.actions.act_window" id="property_type_window">
      <field name="name">Property Types</field>
      <field name="res_model">property.type</field>
      <field name="view_mode">tree,form</field>
    </record>



    <menuitem name="Property" id="property_type_menu"
              parent="stock.menu_stock_config_settings"
              action="fhfl_sales_custom.property_type_window"
              groups="stock.group_stock_user,stock.group_stock_manager"
              sequence="2"/>

    <record id="fhfl_sales_custom_product_temp_tree_view" model= "ir.ui.view">
            <field name="name">fhfl.sales.product.temp.tree.view</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_variant_count']" position="before">
                    <header>
                        <button name="action_create_sales_order" type="object" string="Create Sales Order"/>
                    </header>
                </xpath>
                <xpath expr="//field[@name='default_code']" position="after">
                    <field name="estate_id" optional="show"/>
                </xpath>
            </field>
    </record>

    <record id="fhfl_sales_custom_product_template_form_view" model= "ir.ui.view">
			<field name="name">fhfl.sales.product.template.form.view</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_form_view"/>
			<field name="arch" type="xml">

				<xpath expr="//div[@name='options']" position="inside">
					<div>
                        <field name="is_property"/>
                        <label for="is_property"/>
                    </div>
                </xpath>

                <xpath expr="//notebook" position="inside">
					<page string="Property Details" attrs="{'invisible':[('is_property','=',False)]}" name="property_details">
                        <group>
                            <group>
                                <field name="estate_id"/>
                                <field name="property_types"/>
                            </group>
                            <group>
                                <field name="sqm_unit"/>
                                <field name="prod_ref_id"/>
                            </group>
                        </group>
                    </page>
                </xpath>

			</field>
		</record>


